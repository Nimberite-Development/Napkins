#Colour -> Enum[VInt32]:
#  Pink, Blue, Red, Green, Yellow, {5} Purple, White

BossBar(1, 0x0A) -> Client:
  uuid: UUID
  action: Enum[VInt] ->
    {0} Add:
      title: TextComponent
      health: Float
      colour: Colour
      division: None # Similarly defined like the 'Colour' definition

    {1} Remove: None

    {2} UpdateHealth:
      health: Float

    {3} UpdateTitle:
      title: TextComponent

    {4} UpdateStyle:
      colour: Colour
      dividers: None

    {5} UpdateFlags:
      flags: UByte