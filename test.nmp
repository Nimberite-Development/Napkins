Colour -> Enum[VInt32]:
  Pink, Blue, Red, Green, Yellow, {5} Purple, White

Divisions -> Enum[VInt32]:
  Zero, Six, Ten, Twelve, Twenty

BossBarAction -> Enum[VInt32]:
  Add, Remove, UpdateHealth, UpdateTitle, UpdateTitle

BossBar(1, 0x0A) -> Client:
  uuid: UUID
  action: BossBarAction ->
    Add:
      title: TextComponent
      health: Float
      colour: Colour
      division: Divisions

    Remove: Null

    UpdateHealth:
      health: Float

    UpdateTitle:
      title: TextComponent

    UpdateStyle:
      colour: Colour
      dividers: Null # Not sure what this is supposed to be

    UpdateFlags:
      flags: UByte